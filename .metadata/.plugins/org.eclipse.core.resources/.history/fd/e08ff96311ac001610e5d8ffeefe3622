package ss.week1;

public class Lamp {
	
	public static final int LOW = 1;
	public static final int MED = 2;
	public static final int HIGH = 3;
	
	private int light;
	
	public Lamp() {
		this.light = LOW;
		
	}
	
	public int light () {
		return light;
	}
	
	public void switchlight(){
		light = (light + 1);
	}
	
	public static void main (String[] args) {
		Lamp lamp1 = new Lamp();
		Lamp lamp2 = new Lamp();
		lamp2 = lamp1 -4 ;
		lamp1.switchlight();
		System.out.println(lamp1.light());
		System.out.println(lamp2.light());
	}
}
