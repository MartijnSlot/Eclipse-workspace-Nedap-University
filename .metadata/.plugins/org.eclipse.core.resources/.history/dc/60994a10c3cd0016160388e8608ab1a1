package week7.threads;

public class TestConsole extends Thread {
	
	private String name;

	public TestConsole(String name){
		this.name = name;
	}
	
	public void run(){
		sum();
	}
	
	private void sum(){
		int int1 = Console.readInt(name + ": int1:");
		int int2 = Console.readInt(name + ": int1:");
		Console.println("the sum of " + int1 + "and " + int2 + "is " + (in1 + int2));
	}
}
